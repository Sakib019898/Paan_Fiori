<mvc:View controllerName="sap.ui.demo.products.controller.Detailproduct"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page 
		title="Product Overview"
        showNavButton="true"
		navButtonPress="onNavBack">
		<ObjectHeader id="detail"/>
		<RatingIndicator class="sapUiSmallMarginBeginEnd" id="rating" editable="false" iconSize="2em"></RatingIndicator>
		<Button id="button" class="sapUiSmallMarginBeginEnd" text="Edit Rating!" press="editOdata"/>
	</Page>
</mvc:View>